// Consider adding the 1918 zones here, if they are not used in your
// organization
// definir la zone racine 
zone "domaineperso.tn" {
      type master ;
      file "/etc/bind/db.domaineperso.tn";
       };

zone "56.168.192.in-addr.arpa" {
      type master ;
      file "/etc/bind/db.reverse.tn";
       };
zone "2.0.10.in-addr.arpa" {
      type master ;
      file "/etc/bind/db.ldap.tn";
       };
zone "0.0.127.in-addr.arpa" {
      type master ;
      file "/etc/bind/db.openvpn.tn";
       };

  
